<table cellspacing="30px" id="table2" class="tablesorter table-striped">
    <thead>
    <tr>
        <th>日期
        </th>
        <th>渠道包名
        </th>
        <th>推广计划
        </th>
        <th>计划消费
        </th>
        <th>展现
        </th>
        <th>点击
        </th>
        <th>点击率
        </th>
        <th>点击单价
        </th>
        <th>
            按钮展现
        </th>
        <th>
            按钮点击
        </th>
        <th>按钮消费
        </th>
        <th>按钮展现占比
        </th>
        <th>按钮消费占比</th>
        <th>落地页消费</th>
        <th>落地页激活</th>
        <th>落地页激活成本</th>
        <th>按钮激活</th>
        <th>按钮激活成本</th>
        <th>计划激活</th>
        <th>计划激活成本</th>
        <th>备注</th>
    </tr>
    </thead>
    <tbody class="table1 list1">
    <% for (var i=0; i<data.length; i++) { %>
    <tr>
        <td class="date"><%=data[i].date %></td>
        <td class="qudao">无</td>
        <td class="tui_jihua"><%=data[i].name %></td>
        <td class="ji_cost"><%=data[i].cost %></td>
        <td class="ji_view"><%=data[i].view %></td>
        <td class="ji_pv"><%=data[i].click %></td>
        <td class="ji_pvrate"><%=data[i].click_rate %></td>
        <td class="ji_pvdan"><%=data[i].click_price %></td>
        <td class="ji_btnview"><%=data[i].view_button %></td>
        <td class="ji_btnpv"><%=data[i].click_button %></td>
        <td class="ji_btncost"><%=data[i].button_cost %></td>
        <td class="ji_btnviewb"><%=data[i].button_view_rate %></td>
        <td class="ji_btncostb"><%=data[i].button_cost_rate %></td>
        <td class="ji_pagecost"><%=data[i].h5_cost %></td>
        <td class="ji_pageactive">
            <a href="#" class="ji_pageactive1" data-type="text"><%=data[i].h5_active %></a>
        </td>
        <td class="ji_pageactivecb"><%=data[i].h5_active_chengben %></td>
        <td class="ji_btnactive"><%=data[i].active_button %></td>
        <td class="ji_btnactivecb"><%=data[i].active_button_chengben %></td>
        <td class="ji_active"><%=data[i].jihua_active %></td>
        <td class="ji_activecb"><%=data[i].jihua_active_chengben %></td>
        <td class="remark">
            <a href="#" class="remark1" data-type="textarea">无</a>
        </td>
    </tr>
    <% } %>
    </tbody>

</table>
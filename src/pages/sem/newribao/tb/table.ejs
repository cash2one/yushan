<table cellspacing="30px" id="table1" class="tablesorter table-striped">
    <thead>
    <tr>
        <th style="width: 100px;">日期</th>
        <th style="width: 100px;">系统</th>
        <th>留存</th>
        <!--<th>实际消费</th>
        <th>下载数</th>
        <th>激活数</th>
        <th class="tou"><a href="#" class="tou_liu" data-type="text" style="color: #fff;">留存</a></th>
        <th>下载率</th>
        <th>激活率</th>
        <th class="lcl">留存率</th>
        <th>下载成本</th>
        <th>激活成本</th>
        <th class="liucb">留存成本</th>
        <th>账户消费</th>
        <th>展现</th>
        <th>点击</th>
        <th>点击率</th>
        <th>点击单价</th>
        <th>落地页总激活</th>
        <th>按钮总激活</th>
        <th>账户余额</th>
        <th>返点</th>
        <th>备注</th>-->
        <th>情况</th>
    </tr>
    </thead>
    <tbody class="table1 list">
    <% for (var j=0; j<data.length; j++) { %>
    <tr data-id="<%=data[j].date %>">
        <td class="time" style="width: 100px;"><%=data[j].date %></td>
        <td class="xitong" style="width: 100px;">安卓/Ios</td>
        <td>
            <p><span>实际消费:</span><span class="acsum"><%=data[j].turecost %></span> <span>账户消费:</span><span class="some_cost"><%=data[j].cost %></span></p>
            <p><span>账户余额:</span><span class="some_remainder"><%=data[j].balance %></span></p>
            <p><span>留存/新购:</span><span class="liu_cun"><a href="#" class="liucun1" data-type="text"><%=data[j].liucun %></a></span> <span>留存/新购率:</span><span class="liucun_rate"><%=data[j].liucun_rate +"%"%></span></p>
            <p><span>留存/新购成本:</span><span class="liucun_cb"><%=data[j].liucun_chengben %></span><span>点击:</span><span class="pv"><%=data[j].pv %></span></p>
            <p><span>点击率:</span><span class="pv_rate"><%=data[j].pv_rate +"%"%></span> <span>点击单价:</span><span class="pv_dan"><%=data[j].click_price %></span></p>
        </td>
        <td>
            <p><span>下载数:</span><span class="down_sum"><%=data[j].download_total %></span> <span>下载率:</span><span class="down_rate"><%=data[j].download_rate +"%" %></span></p>
            <p><span>激活数:</span><span class="active_sum"><%=data[j].active_total %></span> <span>激活率:</span><span class="active_rate"><%=data[j].active_rate+"%" %></span></p>
            <p><span>下载成本:</span><span class="down_cb"><%=data[j].download_chengben %></span> <span>激活成本:</span><span class="active_cb"><%=data[j].active_chengben %></span></p>
            <p><span>展现:</span><span class="view"><%=data[j].view %></span> <span>落地页总激活:</span><span class="page_active"><%=data[j].lp_active %></span></p>
            <p><span>按钮总激活:</span><span class="btn_active"><a href="#" class="btn_active1" data-type="text"><%=data[j].button_active %></a></span> <span>返点:</span><span class="re_point"><%=data[j].fd %></span></p>
            <p><span>备注:</span><span class="shuoming"><a href="#" class="beizhu" data-type="textarea">无</a></span></p>
        </td>
    </tr>
    <% } %>

    </tbody>

</table>